import java.util.*;

public class rough {

    public static void sumOfOddnumbers(int n){
        
        int sum=0;
        for(int i=1; i<=n; i++){
            if(i%2 != 0){
                sum += i;
            }
        }
        System.out.println("sum of odd numbers is : "+sum);
        
    }
     

    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        
        System.out.println("enter the value of n");
        int k = sc.nextInt();

        sumOfOddnumbers(k);
    }
}
