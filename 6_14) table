import java.util.*;

public class rough {

    public static void table(int n){
        for(int i=1; i<=10; i++){
            System.out.print(n+" x "+ i );
            System.out.println(" = "+n*i);
        }
    }
     

    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the number of table");
        int k = sc.nextInt();

        table(k);
    }
}
