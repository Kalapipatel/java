import java.util.*;

public class rough {

    public static void eligible(int a){
        
        if(a>=18){
            System.out.println("you are eligible for vote");
        }
        else{
            System.out.println("not elegible for vote");
        }
    }

    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        
        int age = sc.nextInt();

        eligible(age);
    }
}
