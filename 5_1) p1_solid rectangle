import java.util.*;

public class rough{
    public static void main(String args[]){

        int r = 4;
        int c = 5;

        //outer loop
        for(int i=1; i<=r; i++){
            for(int j=1; j<=c; j++){
                System.out.print("*");
            }
            System.out.println("");
        }

    }
}
